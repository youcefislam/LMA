<%- include("_header") %> 
<h1>
    Welcome to Learn About Me!
</h1>
<% users.forEach(user => { %>
    <div class="card my-3" >
        <div class="card-body">
            <div class="card-title">
                <a href="/users/<%= user.username %>"><%= user.name() %> </a>
            </div>
            <% if (user.bio) { %>
             <p class="card-text p-1"><%= user.bio %> </p>
            <% } %>
        </div>
    </div>
<% }) %>
<%- include("_footer") %> 